  method PERSONNELSET_DELETE_ENTITY.

DATA : LS_KEY_TAB TYPE LINE OF /IWBEP/T_MGW_NAME_VALUE_PAIR   ,
 PERSID TYPE ZDAHILI .
    READ TABLE IT_KEY_TAB INTO LS_KEY_TAB WITH KEY NAME = 'Persid' .
      IF SY-SUBRC EQ 0 .
          PERSID  = LS_KEY_TAB-VALUE .
          DELETE FROM ZHR_T_PERS WHERE PERSID = PERSID .
      ENDIF .

  endmethod.